{
  "name": "homicide-media-tracker",
  "version": "1.2.0",
  "description": "An application to track (homicide) media.",
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "typescript",
    "ts",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulianVJacobs/Homicide-Media-Tracker.git"
  },
  "license": "MIT",
  "contributors": [
    {
      "name": "Max Brenner",
      "url": "https://github.com/MaxB543/"
    },
    {
      "name": "Julian Jacobs",
      "url": "https://github.com/JulianVJacobs"
    }
  ],
  "main": "./dist/main/main.js",
  "scripts": {
    "all.package": "npm run build.electron && npm run package",
    "app.prepare": "node scripts/prepare-app.js",
    "build": "next build && node scripts/generate-public-cache.js",
    "build.electron": "npm run build && npm run build.main && npm run build.preload && npm run app.prepare",
    "build.preload": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config webpack.preload.config.js",
    "build.main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config webpack.main.config.js",
    "build-and-package.electron.all": "npm run build.electron && npm run all.package",
    "build-and-package.electron.linux-appimage": "npm run build.electron && npm run linux-appimage.package",
    "build-and-package.electron.windows-portable": "npm run build.electron && npm run windows-portable.package",
    "build-and-package.electron.macos-dmg": "npm run build.electron && npm run macos-dmg.package",
    "build-and-start.web": "npm run build && next start",
    "dev": "next dev",
    "dev.electron": "cross-env NODE_ENV=development electron dist/main/main.js",
    "start.electron": "npm run dev.electron",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && npm run dev.electron\"",
    "start.web": "next start",
    "lint": "next lint",
    "lint.fix": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "package.all": "electron-builder build --win --mac --linux --publish never",
    "package.electron": "ts-node scripts/check-native-dep.js && electron-builder install-app-deps",
    "package.linux-appimage": "electron-builder build --linux --publish never",
    "package.windows-portable": "electron-builder build --win --publish never",
    "package.macos-dmg.": "electron-builder build --mac --publish never",
    "package.web": "npm run build",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "tunnel": "lt --port 3000 --subdomain news-media-tracker-demo",
    "test": "jest"
  },
  "dependencies": {
    "@electron/notarize": "^2.3.2",
    "@libsql/client": "^0.5.6",
    "dexie": "^4.2.0",
    "drizzle-orm": "^0.30.10",
    "electron-debug": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^6.1.4",
    "localtunnel": "^2.0.2",
    "next": "^14.2.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.21.2"
  },
  "optionalDependencies": {
    "@libsql/darwin-arm64": "^0.5.6",
    "@libsql/darwin-x64": "^0.5.6",
    "@libsql/linux-arm64": "^0.5.6",
    "@libsql/linux-x64": "^0.5.6",
    "@libsql/win32-x64": "^0.5.6"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@svgr/webpack": "^8.1.0",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-test-renderer": "^18.0.1",
    "@types/uuid": "^9.0.8",
    "@types/webpack-bundle-analyzer": "^4.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "chart.js": "^4.4.1",
    "concurrently": "^8.2.1",
    "core-js": "^3.32.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "detect-port": "^1.5.1",
    "drizzle-kit": "^0.20.18",
    "electron": "^38.0.0",
    "electron-builder": "^24.13.3",
    "electron-devtools-installer": "^3.2.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.1.0-0",
    "eslint-config-next": "14.2.5",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-import-resolver-webpack": "^0.13.7",
    "eslint-plugin-compat": "^4.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nth-check": "^2.1.1",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.0.3",
    "react-bootstrap": "^2.10.0",
    "react-chartjs-2": "^5.2.0",
    "react-refresh": "^0.14.0",
    "react-select": "^5.8.0",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^10.0.3",
    "rimraf": "^5.0.1",
    "sass": "^1.67.0",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.2.2",
    "url-loader": "^4.1.1",
    "uuid": "^9.0.1",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.9.0"
  },
  "browserslist": [
    "electron >= 31.0.0"
  ],
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "build": {
    "productName": "HomicideMediaTracker",
    "appId": "org.wits.HomicideMediaTracker",
    "asar": true,
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "asarUnpack": [
      "**/*.{node,dll,dylib,so}",
      ".next/**/*",
      "server.js",
      "package.json",
      "node_modules/**/*"
    ],
    "files": [
      "dist/**/*",
      "server.js",
      ".next/**/*",
      "package.json",
      "node_modules/@libsql/**/*",
      "node_modules/libsql/**/*",
      "node_modules/drizzle-orm/**/*"
    ],
    "extraResources": [
      {
        "from": "node_modules/@libsql/",
        "to": "resources/libsql"
      },
      "./assets/**"
    ],
    "mac": {
      "sign": "scripts/notarize.js",
      "notarize": false,
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "dir",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.ico",
      "requestedExecutionLevel": "asInvoker",
      "artifactName": "HomicideMediaTracker-${version}-portable.exe",
      "sign": false,
      "signAndEditExecutable": false,
      "certificateFile": false,
      "certificatePassword": false
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "publish": {
      "provider": "github",
      "owner": "electron-react-boilerplate",
      "repo": "electron-react-boilerplate"
    }
  },
  "collective": {
    "url": "https://opencollective.com/electron-react-boilerplate-594"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=14.x",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=7.x",
      "onFail": "error"
    }
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}
